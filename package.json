{
  "name": "wevolv3-website",
  "version": "2.0.0",
  "description": "Wevolv3 Official Website - Digital Reality with Desktop Faithful Mobile Experience",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:blog": "next dev -p 3000",
    "build": "vite build",
    "build:blog": "next build",
    "build:studio": "cd sanity && npm install && npm run build",
    "preview": "vite preview",
    "preview:blog": "next start",
    "studio": "cd sanity && npm run dev",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "wevolv3",
    "digital-reality",
    "react",
    "vite",
    "tailwindcss",
    "desktop-faithful-mobile",
    "responsive-design",
    "glass-morphism",
    "pwa"
  ],
  "author": {
    "name": "Wevolv3 Team",
    "email": "contato@wevolv3.com",
    "url": "https://wevolv3.com"
  },
  "license": "MIT",
  "homepage": "https://wevolv3.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/wevolv3/wevolv3-website.git"
  },
